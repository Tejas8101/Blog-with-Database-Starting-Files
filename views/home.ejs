<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <form action="/compose" method="get">
        <button type="submit" class="btn btn-outline-primary btn-lg">Compose</button>
    </form>



    <%  posts.forEach(function(post){ %>

        <h1>
            <%=post.title%>
        </h1>
        <p>
            <%=post.content%>
                <a href="/posts/<%=post._id%>"></a>
        </p>


        <% }) %>


            <%- include("partials/footer"); -%>